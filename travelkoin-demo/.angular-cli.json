{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "version": "1.0.0",
    "name": "travelkoin-demo"
  },
  "apps": [
    {
      "serviceWorker": true,
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.scss",
        "extras/helpers/animate.css",
        "extras/helpers/backgrounds.css",
        "extras/helpers/boilerplate.css",
        "extras/helpers/border-radius.css",
        "extras/helpers/grid.css",
        "extras/helpers/page-transitions.css",
        "extras/helpers/spacing.css",
        "extras/helpers/typography.css",
        "extras/helpers/utils.css",
        "extras/helpers/colors.css",
        "extras/elements/badges.css",
        "extras/elements/buttons.css",
        "extras/elements/content-box.css",
        "extras/elements/dashboard-box.css",
        "extras/elements/forms.css",
        "extras/elements/images.css",
        "extras/elements/info-box.css",
        "extras/elements/invoice.css",
        "extras/elements/loading-indicators.css",
        "extras/elements/menus.css",
        "extras/elements/panel-box.css",
        "extras/elements/response-messages.css",
        "extras/elements/responsive-tables.css",
        "extras/elements/ribbon.css",
        "extras/elements/social-box.css",
        "extras/elements/tables.css",
        "extras/elements/tile-box.css",
        "extras/elements/timeline.css",
        "extras/icons/traveliko/styles.css",
        "extras/snippets/chat.css",
        "extras/snippets/files-box.css",
        "extras/snippets/login-box.css",
        "extras/snippets/notification-box.css",
        "extras/snippets/progress-box.css",
        "extras/snippets/todo.css",
        "extras/snippets/user-profile.css",
        "extras/snippets/mobile-navigation.css",
        "extras/applications/mailbox.css",
        "extras/frontend-elements/cta-box.css",
        "extras/frontend-elements/feature-box.css",
        "extras/frontend-elements/footer.css",
        "extras/frontend-elements/hero-box.css",
        "extras/frontend-elements/icon-box.css",
        "extras/frontend-elements/testimonial-box.css",
        "extras/themes/admin/color-schemes/default.css",
        "extras/themes/frontend/layout.css",
        "extras/themes/frontend/color-schemes/default.css",
        "extras/themes/components/default.css",
        "extras/themes/components/border-radius.css",
        "extras/frontend-elements/blog.css",
        "extras/helpers/responsive-elements.css",
        "extras/helpers/admin-responsive.css",
        "extras/helpers/frontend-responsive.css"
      ],
      "scripts": [],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "local": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      },
      "stylePreprocessorOptions": {
        "includePaths": [
          "styles"
        ]
      }
    }
  ],
  "addons": [],
  "packages": [],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "component": {
      "inlineTemplate": false,
      "spec": true
    }
  }
}
